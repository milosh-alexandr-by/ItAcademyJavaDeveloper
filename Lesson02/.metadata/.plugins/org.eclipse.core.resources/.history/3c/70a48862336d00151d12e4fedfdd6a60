
import java.util.*;

public class DayCalendar {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
//		sc.useLocale(Locale.US);
		System.out.println("¬ведите дату в формате: число.мес€ц.год. (Ќарпример: 9.05.1945)");
		System.out.print("Your date: ");
		String date = sc.nextLine();
		System.out.println(date);
		sc.close();
		
		String[] numbers = date.split("-");
		
		Integer d = Integer.valueOf(numbers[0]);    // число мес€ца
		System.out.println("d = " + d);
		Integer M = Integer.valueOf(numbers[1]);    // номер мес€ца
		
		int m = 0;             // упор€дочиваем мес€цы согласно ƒревнему –иму
		if (M==1) {
			m = 11;
		}
		else {
			if (M==2) {
				m = 12;
			}
			else {
				m = M-2;
			}
		}
		
		Integer y = Integer.valueOf(numbers[2]);     // номер года согласно ƒревнему –иму
		int Y = y%100;
		if ((m==11) | (m==12)) {
			Y = Y-1;
		}
		
		int c = y/100;
		int summ1 =(13*m-1)/5;
		int summ2 = Y/4;
		int summ3 = c/4;
		
		int day = (d + summ1 + Y + summ2 + summ3 - (2*c)+777)%7;
		System.out.println("„исло day равно: " + day);
		
	}

}
