
public class TableOfSquares {

	public static void main(String[] args) {

		/* 
		   Ќаписать таблицу умножени€ не использу€ массивов
		*/

		System.out.print("  ");
		for (int i=1; i<=4; i++) {
			System.out.print(i + " ");
		}
		System.out.println("5");
		for (int i=1; i<=11; i++) {
			System.out.print("_");
		}	
	}

}
