
import java.util.*;

public class DayCalendar {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		sc.useLocale(Locale.US);
		System.out.println("¬ведите дату в формате: число.мес€ц.год. (Ќарпример: 9.05.1945)");
		System.out.print("¬аша дата: ");
		String date = sc.nextLine();
		System.out.println(date);
		sc.close();
		
		String[] numbers = date.split(".");
		System.out.println(numbers[0]);
		System.out.println(numbers[1]);
		System.out.println(numbers[2]);
		
		Integer d = Integer.valueOf(numbers[0]);    // число мес€ца
		System.out.println("d = " + d);
		Integer M = Integer.valueOf(numbers[1]);    // номер мес€ца
		
		int m = 0;
		if (M==1) {
			m = 11;
		}
		else {
			if (M==2) {
				m = 12;
			}
			else {
				m = M-2;
			}
		}
//		System.out.println("m = " + m);    // номер мес€ца в ƒревнем –име
		
		Integer y = Integer.valueOf(numbers[2]);     // номер года
		int Y = y%100;
		if ((m==11) | (m==12)) {
			Y = Y-1;
		}
//		System.out.println("y = " + Y);
		
		int c = y/100;
//		System.out.println("c = " + c);
		
		
		int summ1 =(13*m-1)/5;
		int summ2 = Y/4;
		int summ3 = c/4;
		
//		Integer chislo = Integer.valueOf(numbers[0]);
//		System.out.println("„исло: " + chislo);
//		System.out.println("ћес€ц: " + M);
//		System.out.println("√од: " + y);
//		
		int day = (d + summ1 + Y + summ2 + summ3 - (2*c)+777)%7;
		System.out.println("„исло day равно: " + day);

		
		int day2 = (d + (13*m-1)/5 + Y + Y/4 + c/4 - (2*c)+777)%7;
		System.out.println("„исло day равно: " + day2);
		
		
		
		
	}

}
