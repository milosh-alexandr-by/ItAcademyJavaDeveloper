
import java.util.Scanner;

public class SumOfSeries {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		Integer n= sc.nextInt();
		System.out.println("n = " + n);
		double sum = 0;
		for (double i=1; i<=n; i++) {
			sum = sum + 1/i;
			System.out.println(sum);
		}
		
		System.out.println(sum);
		
	}

}
