		int i = 1;
		boolean boo = false;
		
		do {
			Scanner sc = new Scanner(System.in);
			System.out.println(sc);
			System.out.print("Введите строку с номером " + i + ": ");
			String str = sc.nextLine();
			System.out.println("Выводим str:" + str);
			i++;
			boo = sc.hasNextDouble();
			System.out.println("Выводим str:" + str);
			System.out.println("Выводим boo:" + boo);
		} while( boo == false);
		
