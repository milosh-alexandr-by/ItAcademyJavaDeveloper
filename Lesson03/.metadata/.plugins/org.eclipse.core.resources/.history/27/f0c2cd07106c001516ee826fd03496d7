
public class NullsToEnd {

	public static void main(String[] args) {

		int ar[] = new int[10];
		for (int i=0; i<10; i++) {
			ar[i] = (int)((Math.random()*5));
		}
		
		System.out.print("ћассив: ");
		for (int i=0; i<10; i++) {
			if (i==9) {
				System.out.println(ar[i] + "."); break;
			}
			System.out.print(ar[i] + ", ");
		}
		
		// подсчитаем количество нулей
		int sum = 0;
		for (int i=0; i<10; i++) {
			if (ar[i]==0) {
				sum = sum + 1;
			}
		}
		System.out.println(" оличество нулей: " + sum);
		
		int num = 0;
		for (int i=0; i<10; i++) {
			if (ar[i]==0) continue;
			num = num +1;
		}

		System.out.println("num = " + num);

		// у нас sum нулей, значит надо сделать ch==sum сдвигов
//		for (int ch=0; ch<=sum; ch++) {                
//			for (int i=0; i<6; i++) {
//				if (ar[i]==0) {
//					for (int k=i; k<=(5-i); k++){
//					ar[k] = ar[k+1];
//					
//					}
//				}
//			}
//			
//		}

	}

}
