
public class MatrixWithZero {

	public static void main(String[] args) {

		Integer[][] matrix = new Integer[5][4];
		for (int i=0; i<5; i++) {
			for (int j=0; j<4; j++) {
				matrix[i][j] = (int) (Math.random()*5);
			}
		}
		for (int i=0; i<5; i++) {
			for (int j=0; j<4; j++) {
				System.out.printf("%4d", matrix[i][j]);
			}
		System.out.println();	
		}

	}

}
