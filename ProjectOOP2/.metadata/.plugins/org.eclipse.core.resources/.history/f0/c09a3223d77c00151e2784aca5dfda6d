import java.util.Comparator;

public class SortedByFuelConsumption implements Comparator<TaxiCars> {
	
public int compare(TaxiCars obj1, TaxiCars obj2) {
		
		int fc1 = obj1.getPrice();
		int fc2 = obj2.getPrice();
		
		if (fc1>fc2) {
			return 1;
		}
		else if (fc1<fc2) {
			return -1;
		}
		else {
			return 0;
		}
		
		
		
	}

}
